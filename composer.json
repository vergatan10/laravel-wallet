{
    "name": "vergatan10/laravel-wallet",
    "description": "Wallet module for Laravel with deposit, withdraw, and transfer features",
    "type": "library",
    "license": "MIT",
    "autoload": {
      "psr-4": {
        "Vergatan10\\Wallet\\": "packages/laravel-wallet/src/"
      }
    },
    "extra": {
      "laravel": {
        "providers": [
          "Vergatan10\\Wallet\\WalletServiceProvider"
        ]
      }
