{
    "name": "vergatan10/laravel-wallet",
    "description": "Wallet module for Laravel with deposit, withdraw, and transfer features",
    "type": "library",
    "license": "MIT",
    "autoload": {
      "psr-4": {
        "Vergatan10\\Wallet\\": "src/",
        "Vergatan10\\Wallet\\Tests\\": "tests/"
      }
    },
    "extra": {
      "laravel": {
        "providers": [
          "Vergatan10\\Wallet\\WalletServiceProvider"
        ]
      }
    },
    "require": {
      "php": "^8.1",
      "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "orchestra/testbench": "^10.3"
    }
}
